        // START SEND AND ADVISOR HOME POPUP CLICK FUNCTION , ABOUT US SEND AND ADVISOR

        //$('[id$=btnSubmit]').click(function () {
        $('#btnSubmit').on('click', function () {
            var ctrName = $('[id$=txtName]');
            var ctrMobile = $('[id$=txtMobile]');
            var ctrEmail = $('[id$=txtEmailAd]');
            var ctrAddress = $('[id$=txtAddress]');
            var ctrCaptcha = grecaptcha.getResponse(captcha13).length;

            if (!validateEmptyControl(ctrName, 'Incomplete Information!!\nEnter your name'))
                return false;
            if (!validateEmptyControl(ctrMobile, 'Incomplete Information!!\nEnter Mobile number'))
                return false;
            //if (ctrMobile.val().length < 10) {
            //    alert("Incorrect Information!!\nPlease enter valid Mobile number"); ctrMobile.focus(); return false;
            //}
            if ($("#txtMobile").val() == "6666666666" || $("#txtMobile").val() == "7777777777" || $("#txtMobile").val() == "8888888888" || $("#txtMobile").val() == "9999999999") {
                alert("Incorrect Information!!\nPlease enter valid Mobile number"); $("#txtMobile").focus(); $("#txtMobile").val(''); return false;
            }
            if (!validateEmptyControl(ctrEmail, 'Incomplete Information!!\nEnter Email Id'))
                return false;
            if (ctrEmail.val().length < 6) {
                alert("Incorrect Information!!\nPlease enter valid email"); ctrEmail.focus(); return false;
            }
            if (ctrCaptcha <= 0) {
                alert("Please fill the captcha.");
                return false;
            }
            else {
                var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,6})?$/;
                if (!emailReg.test(ctrEmail.val())) {
                    alert("Incorrect Information!!\nPlease enter valid email"); ctrEmail.focus();
                    return false;
                }
            }

            if (!validateEmptyControl(ctrAddress, 'Enter Address'))
                return false;
            // $('.overlay').show();

            var data = new FormData();
            data.append("Name", ctrName.val().trim());
