            try {

                var mouseEvent = document.createEvent("MouseEvent");
                mouseEvent.initMouseEvent("click", true, true, window, 0, 0, 0, 0, 0, false, false, false, false, 0, null);


                //var mouseEvent = new MouseEvent('onmousedown', {
                //    'view': window,
