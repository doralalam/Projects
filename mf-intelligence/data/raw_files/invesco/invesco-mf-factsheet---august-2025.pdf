    <script>

        function CreatePopupCookie(cookieName, value) {
            var expiration_date = new Date();
            var cookie_string = '';
            expiration_date.setFullYear(expiration_date.getFullYear() + 1);
            // Build the set-cookie string:
            cookie_string = cookieName + "=" + value + "; path = /; expires=" + expiration_date.toUTCString();
            // Create or update the cookie:
            document.cookie = cookie_string;
        }

        // Get the CookieModal
        var modal = document.getElementById("cookiemodal");

        // Get the <span> element that closes the modal
        var span = document.getElementsByClassName("cookieclose")[0];

        // When the user clicks the button, open the modal
        if (getCookie('CookiePolicy') != 2) {
            modal.style.display = "block";

            CreatePopupCookie('CookiePolicy', 1);

            //createCookie('CookiePolicy', 1, 1);
        }
        else {
            modal.style.display = "none";
        }

        // When the user clicks on <span> (x), close the modal
        span.onclick = function () {
            modal.style.display = "none";
            CreatePopupCookie('CookiePolicy', 2);
            //createCookie('CookiePolicy', 2, 1);
        }

        // When the user clicks anywhere outside of the modal, close it
        window.onclick = function (event) {
            if (event.target == modal) {
                modal.style.display = "none";
            }
        }

        //Hiding Phone Icon from PMS site
        window.onload = function (e) {
            if (location.href.search('/pms/') > -1) {
                $('.contactUs-icon').hide();
            }
        }

    </script>
