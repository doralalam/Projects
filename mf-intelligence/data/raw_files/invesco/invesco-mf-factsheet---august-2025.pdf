    <script>
        var CSV = '';
        var flagdownload = true;
        var dividendcount = 0;
        var title = '';
        var pagename = '';
        //var execount = 0;
        //var columndata = '';

        function JSONToCSVConvertor(JSONData, ReportTitle, ShowLabel) {
            CSV = '';
            flagdownload = true;
            flagtemp = true;
            title = '';
            //If JSONData is not an object then JSON.parse will parse the JSON string in an Object
            var arrData = typeof JSONData != 'object' ? JSON.parse(JSONData) : JSONData;
            //alert(arrData);

            CSV = '';

            //Set Report title in first row or line

            CSV += ReportTitle + '\r\n\n'; //DateFile Text in file row 1 col 1 cell.

            if (ReportTitle == "Historical NAV") {

                var PlanScheme = $('#PlanNameNSchemeName').text();
                // alert(PlanScheme);
                CSV += PlanScheme + '\r\n\n';
            }

            //This condition will generate the Label/Header
            if (ShowLabel) {
                var row = "";

                //This loop will extract the label from 1st index of on array
                for (var indexs in arrData[0]) {
                    ////alert(arrData[0][index]);
                    //Now convert each value to string and comma-seprated

                    row += indexs + ',';


                }
            }

            //columndata += row
            row = row.slice(0, -1);
            var array = row.split(",");
            $.each(array, function (i) {
                if (array[i] == "DividendDeclared") {
                    pagename = array[i];
                }
            });
            //append Label row with line break
            CSV += row + '\r\n';
            //columndata = columndata.slice(0, -1);
            //  }
            //1st loop is to extract each row
            for (var i = 0; i < arrData.length; i++) {
                var row = "";

                //2nd loop will extract each column and convert it in string comma-seprated
                for (var index in arrData[i]) {

                    //alert(arrData[i][index]);
                    //alert(arrData[i]);

                    if (typeof arrData[i][index] == 'object') {
                        //alert("I am an object 1");

                        NestedObjectProcessing(arrData[i][index], ShowLabel)
                    }
                    else {
                        //alert("I am an data 1.");
                        row += '"' + arrData[i][index] + '",';
                        title = row;
                        //alert(row);
                        if (index == 'FundName') {
                            if (flagdownload == false) {
                                row.slice(0, row.length - 1);
                                CSV += row + '\r\n';
                            }
                        }
                        else if (index == 'Date') {
                            if (flagdownload == false) {
                                row.slice(0, row.length - 1);
                                CSV += row + '\r\n';
                            }
                        }
                        else {
                            row.slice(0, row.length - 1);
                            CSV += row + '\r\n';
                        }

                    }
                    // row += '"' + arrData[i][index] + '",';
                }


            }

            if (CSV == '') {
                //  return "N";
                //alert("Invalid data");
                return;

            }

            if (msieversion()) {
                download_IE(CSV, 'dowload.csv', 'text/csv');
                //downloadEdgeCSV(CSV, 'dowload.csv', 'text/csv');

            } else {
                //Generate a file name
                var fileName = "MyReport_";
                //this will remove the blank-spaces from the title and replace it with an underscore
                fileName += ReportTitle.replace(/ /g, "_");

                //Initialize file format you want csv or xls
                var uri = 'data:text/csv;charset=utf-8,' + escape(CSV);

                // Now the little tricky part.
                // you can use either>> window.open(uri);
                // but this will not work in some browsers
                // or you will not get the correct file extension

                //this trick will generate a temp <a /> tag
                var link = document.createElement("a");
                link.href = uri;

                //set the visibility hidden so it will not effect on your web-layout
                link.style = "visibility:hidden";
                link.download = fileName + ".csv";

                //this part will append the anchor tag and remove it after automatic click
                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);
            }

        }

        function NestedObjectProcessing(JSONData, ShowLabel) {

            var ReportTitle = "Latest NAV";


            ////alert(arrData2);
            //If JSONData is not an object then JSON.parse will parse the JSON string in an Object
            var arrData2 = typeof JSONData != 'object' ? JSON.parse(JSONData) : JSONData;

            //var CSV = '';
            //Set Report title in first row or line

            //CSV += ReportTitle + '\r\n\n';

            if (pagename == "DividendDeclared" && flagdownload == false && dividendcount == 1 && flagtemp == true) {
                row = "";
                flagdownload = true;
            }

            //This condition will generate the Label/Header
            if (ShowLabel && flagdownload == true) {
                var row = "";
                if (pagename == "DividendDeclared") {
                    row = "";
                    if (dividendcount == 0) {
                        dividendcount = 1;
                    }
                    else {
                        dividendcount = 0;
                    }

                }
                //This loop will extract the label from 1st index of on array
                for (var index in arrData2[0]) {
                    //alert(index);
                    //Now convert each value to string and comma-seprated

                    row += index + ',';

                }

                row = row.slice(0, row.length - 1);

                //append Label row with line break
                CSV += row + '\r\n';
                flagdownload = false;
                title.slice(0, row.length - 1);
                CSV += title + '\r\n';
            }
            if (arrData2) {
                //1st loop is to extract each row
                for (var i = 0; i < arrData2.length; i++) {
                    var row = "";

                    //2nd loop will extract each column and convert it in string comma-seprated
                    for (var index in arrData2[i]) {
                        //alert(arrData2[i][index]);
                        if (typeof arrData2[i][index] == 'object') {
                            //alert("I am an object 2");
                            NestedObjectProcessing(arrData2[i][index], ShowLabel)
                        }
                        else {
                            //alert("I am an data 2.");
                            row += '"' + arrData2[i][index] + '",';
                        }
                        //row += '"' + arrData2[i][index] + '",';
                    }

                    row.slice(0, row.length - 1);

                    //add a line break after each row
                    CSV += row + '\r\n';
                }
            }
        }

        /*To Check for Browser MSIE OR EDGE Browser */
        function msieversion() {
            var ua = window.navigator.userAgent;
            var msie = ua.indexOf("MSIE ");
            var edge = ua.indexOf('Edge/');
            if (msie > 0 || edge > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) // If Internet Explorer, return true
            {
                return true;
            } else { // If another browser,
                return false;
            }
            return false;
        }

        /*START download for IE / Edge Browser */
        var download_IE = function (content, fileName, mimeType) {
            var a = document.createElement('a');
            mimeType = mimeType || 'application/octet-stream';

            if (navigator.msSaveBlob) { // IE10
                return navigator.msSaveBlob(new Blob([content], { type: mimeType }), fileName);
            } else if ('download' in a) { //html5 A[download]
                a.href = 'data:' + mimeType + ',' + encodeURIComponent(content);
                a.setAttribute('download', fileName);
                document.body.appendChild(a);
                setTimeout(function () {
                    a.click();
                    document.body.removeChild(a);
                }, 66);
                return true;
            } else { //do iframe dataURL download (old ch+FF):
                var f = document.createElement('iframe');
                document.body.appendChild(f);
                f.src = 'data:' + mimeType + ',' + encodeURIComponent(content);

                setTimeout(function () {
                    document.body.removeChild(f);
                }, 333);
                return true;
            }
        }



